{
  "schemaVersion": 1,
  "id": "lint",
  "version": "1.2.0",
  "name": "Lint",
  "description": "Adds lots of content to the game to extend gameplay and make it more enjoyable",
  "license": "MIT",
  "authors": [
    "hydos",
    "valoghese",
    "ramidzkh"
  ],
  "icon": "assets/lint/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "io.github.hydos.lint.Lint",
      "io.github.hydos.lint.network.Networking",
      "io.github.hydos.lint.world.structure.Structures",
      "io.github.hydos.lint.world.biome.Biomes",
      "io.github.hydos.lint.container.Containers",
      "io.github.hydos.lint.entity.Entities::initialize",
      "io.github.hydos.lint.resource.block.Blocks::initialize",
      "io.github.hydos.lint.resource.item.Items::initialize"
    ],
    "client": [
      "io.github.hydos.lint.client.LintClient",
      "io.github.hydos.lint.network.ClientNetworking"
    ],
    "cardinal-components-entity": [
      "io.github.hydos.lint.spirit.SpiritComponentInitializer"
    ]
  },
  "custom": {
    "cardinal-components": [
      "lint:spirits"
    ]
  },
  "mixins": [
    "mixins.lint.json"
  ],
  "accessWidener": "lint.aw",
  "depends": {
    "fabric": "*",
    "minecraft": "1.16.x",
    "libgui": ">=3.0.0",
    "cardinal-components-entity": ">=2.5.0"
  }
}
